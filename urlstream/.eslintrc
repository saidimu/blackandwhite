{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "globals": {
    "module": true,
    "define": true,
    "requirejs": true,
    "require": true,
    "describe": true,
    "it": true
  },
  "rules": {
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-use-before-define": 0,
    "new-cap": 2,
    "no-caller": 2,
    "dot-notation": 0,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-eq-null": 2,
    "no-new": 2,
    "no-bitwise": 2,
    "camelcase": 0,
  }
}
